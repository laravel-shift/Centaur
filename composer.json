{
    "name": "srlabs/centaur",
    "description": "An opinionated implementation of Cartalyst's Sentinel for Laravel 5",
    "keywords": [
        "laravel",
        "sentinel",
        "auth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Durham",
            "email": "ryan@stagerightlabs.com",
            "homepage": "http://stagerightlabs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Centaur\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "cartalyst/sentinel": "^5.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "^7.0",
        "mockery/mockery": "^1.3.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Centaur\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "vinkla/hashids": "A Hashids bridge for Laravel"
    }
}
